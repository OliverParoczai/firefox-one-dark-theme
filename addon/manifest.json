{
    "manifest_version": 2,
    "name": "Atom One Dark Theme",
    "version": "1.0",
  
    "description": "The Atom One Dark theme for Firefox.",
  
    "theme": {
      "colors": {
        "bookmark_text":                "#abb2bf",
          
        "button_background_active":     "#0000",
        "button_background_hover":      "#0000",
          
        "icons":                        "#abb2bf",
        "icons_attention":              "#528bff",
          
        "frame":                        "#21252b",
